{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"check": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"clean": {
			"cache": false
		},
		"format": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/**"]
		}
	}
}
